@model IEnumerable<QuanLyKhoLinhKienPC.Models.PhieuNhap>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.NgayNhap)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.TongTien)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.GhiChu)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IsDeleted)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.MaNguoiDungNavigation)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.MaNhaCungCapNavigation)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.NgayNhap)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.TongTien)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.GhiChu)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IsDeleted)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.MaNguoiDungNavigation.MatKhau)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.MaNhaCungCapNavigation.TenNhaCungCap)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.MaPhieuNhap">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.MaPhieuNhap">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.MaPhieuNhap">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
